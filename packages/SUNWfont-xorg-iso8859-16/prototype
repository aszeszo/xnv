# X.Org iso8859-16 fonts package

i copyright
i depend=../common_files/depend.font
i pkginfo
i postinstall
i postremove

!HOME=../usr

d none share							0755 root sys
d none share/fonts						0755 root bin
d none share/fonts/X11						0755 root bin

!search $HOME/share/fonts/X11/100dpi-ISO8859-16
d none share/fonts/X11/100dpi-ISO8859-16			0755 root bin
f none share/fonts/X11/100dpi-ISO8859-16/fonts.dir		0444 root bin

!search $HOME/share/fonts/X11/75dpi-ISO8859-16
