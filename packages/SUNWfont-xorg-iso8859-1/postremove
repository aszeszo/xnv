# These symlinks cannot be put into packages directly, or even with installf,
# since pkgadd/pkgmk/pkgrm can't handle files with = in their pathname.
FONTDIR=../../../usr/X11/lib/X11/fonts
FONTPATHDIR=${BASEDIR}/../etc/X11/fontpath.d

# Links made before build 132
rm -f ${FONTPATHDIR}/misc-ISO8859-1:unscaled:pri=16
rm -f ${FONTPATHDIR}/100dpi-ISO8859-1:unscaled:pri=21
rm -f ${FONTPATHDIR}/75dpi-ISO8859-1:unscaled:pri=26
rm -f ${FONTPATHDIR}/misc-ISO8859-1:pri=86
rm -f ${FONTPATHDIR}/100dpi-ISO8859-1:pri=91
rm -f ${FONTPATHDIR}/75dpi-ISO8859-1:pri=96

# Links made in builds 132 & later
rm -f ${FONTPATHDIR}/misc-ISO8859-1:unscaled:pri=15
rm -f ${FONTPATHDIR}/100dpi-ISO8859-1:unscaled:pri=20
rm -f ${FONTPATHDIR}/75dpi-ISO8859-1:unscaled:pri=25
rm -f ${FONTPATHDIR}/misc-ISO8859-1:pri=85
rm -f ${FONTPATHDIR}/100dpi-ISO8859-1:pri=90
rm -f ${FONTPATHDIR}/75dpi-ISO8859-1:pri=95

exit 0 
