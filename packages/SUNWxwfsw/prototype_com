i pkginfo
i copyright=../copyright
i depend
i i.encodingsdir
i r.encodingsdir
i i.ttmapsdir
i r.ttmapsdir

!HOME=../openwin
!X11HOME=../usr/X11

d none openwin 0755 root bin

!search  $HOME/bin
d none openwin/bin              0755 root bin
f none openwin/bin/mkfontdir   0755 root bin

!search  $HOME/server/lib
d none openwin/server 0755 root bin
d none openwin/server/lib 0755 root bin
s none openwin/server/lib/libfont.so=./libfont.so.1
f none openwin/server/lib/libfont.so.1 0755 root bin
s none openwin/server/lib/libtypesclr.so=./libtypesclr.so.0
f none openwin/server/lib/libtypesclr.so.0 0755 root bin
 

!search	 $HOME/lib
d none openwin/lib				0755 root bin
f none openwin/lib/installalias         0755 root bin

!search  $HOME/lib/X11
