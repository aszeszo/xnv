!include prototype_com

!HOME=../openwin

d none openwin 0755 root bin
!search	 $HOME/lib
d none openwin/lib				0755 root bin

!search  $HOME/lib/X11
d none openwin/lib/X11 				0755 root bin

!search  $HOME/lib/X11/fonts
d none openwin/lib/X11/fonts 0755 root bin

!search  $HOME/lib/X11/fonts/Type1
d none openwin/lib/X11/fonts/Type1 			0755 root bin

!search  $HOME/lib/X11/fonts/Type1/prebuilt
d none openwin/lib/X11/fonts/Type1/prebuilt 0755 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Courier-Bold.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Courier-BoldOblique.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Courier-Oblique.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Courier.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Helvetica-Bold.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Helvetica-BoldOblique.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Helvetica-Oblique.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Helvetica.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Symbol.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Times-Bold.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Times-BoldItalic.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Times-Italic.lepf 0644 root bin
f none openwin/lib/X11/fonts/Type1/prebuilt/Times-Roman.lepf 0644 root bin

