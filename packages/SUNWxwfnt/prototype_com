d none openwin							0755 root bin
d none openwin/lib						0755 root bin
d none openwin/lib/X11						0755 root bin
d none openwin/lib/X11/fonts					0755 root bin
s none openwin/lib/X11/fonts/75dpi=../../../../share/fonts/X11/75dpi
s none openwin/lib/X11/fonts/100dpi=../../../../share/fonts/X11/100dpi
s none openwin/lib/X11/fonts/F3bitmaps=../../../../share/fonts/sun/F3bitmaps
d none openwin/lib/X11/fonts/Type1				0755 root bin
s none openwin/lib/X11/fonts/Type1/sun=../../../../share/fonts/sun/Type1

d none X11							0755 root bin
d none X11/lib							0755 root bin
d none X11/lib/X11						0755 root bin
d none X11/lib/X11/fonts					0755 root bin
s none X11/lib/X11/fonts/F3bitmaps=../../../../share/fonts/sun/F3bitmaps

# These need to be a directory, not a symlink, since other packages install
# subdirectories here - empty fonts.dir left to keep X configurations with
# these paths in the font dir from dying
d none openwin/lib/X11/fonts/misc				0755 root bin
e fontsdir openwin/lib/X11/fonts/misc/fonts.dir			0644 root bin
d none openwin/lib/X11/fonts/TrueType				0755 root bin
e fontsdir openwin/lib/X11/fonts/TrueType/fonts.dir		0644 root bin

