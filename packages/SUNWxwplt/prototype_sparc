!include prototype_com

!HOME=../openwin

#   X Window System 64-bit library software

!search  $HOME/lib/sparcv9
# Symlink removed to prevent compiling new programs - library is left
# for binary compatibility only, and 64-bit should only be delivered on sparc
#s none openwin/lib/sparcv9libXinput.so=./libXinput.so.0
f none openwin/lib/sparcv9/libXinput.so.0       0755 root bin
s none openwin/lib/sparcv9/libdps.so=./libdps.so.5
f none openwin/lib/sparcv9/libdps.so.5 		0755 root bin
s none openwin/lib/sparcv9/libdga.so=./libdga.so.1
f none openwin/lib/sparcv9/libdga.so.1 		0755 root bin
s none openwin/lib/sparcv9/libowconfig.so=./libowconfig.so.0
f none openwin/lib/sparcv9/libowconfig.so.0 	0755 root bin
