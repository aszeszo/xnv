i pkginfo
i copyright=../copyright
i depend

!HOME=../openwin

d none openwin			0755 root bin

!search      $HOME/bin
d none openwin/bin		0755 root bin
f none openwin/bin/accessx	0755 root bin


!search  $HOME/lib
d none openwin/lib 0755 root bin

d none openwin/lib/app-defaults 0755 root bin
!search  $HOME/lib/X11/app-defaults
f none openwin/lib/app-defaults/accessx.uid 0644 root bin

!search  $HOME/lib/X11/locale/C/help
s none openwin/lib/help=./locale/C/help
d none openwin/lib/locale 0755 root bin
d none openwin/lib/locale/C 0755 root bin
d none openwin/lib/locale/C/help 0755 root bin
f none openwin/lib/locale/C/help/accessx.hlp 0644 root bin

!search  $HOME/share/man/man1
s none openwin/man=./share/man
d none openwin/share 0755 root bin
d none openwin/share/man 0755 root bin
d none openwin/share/man/man1 0755 root bin
f none openwin/share/man/man1/accessx.1 0444 root bin
