# ident "@(#)prototype 1.22     08/02/26 SMI"
#
#  ISO-8859-1 (Latin-1) Optional Fonts

i pkginfo
i postinstall
i postremove
i copyright=../copyright
i depend=../depend
i i.fontsdir
i r.fontsdir
i i.fontsalias
i r.fontsalias
i i.encodingsdir
i r.encodingsdir

!HOME=../openwin/lib/X11/fonts

d none openwin 0755 root bin
d none openwin/lib 0755 root bin
d none openwin/lib/X11 0755 root bin

!search            $HOME
d none openwin/lib/X11/fonts 0755 root bin

