# Xprint printing server

i pkginfo
i copyright

!HOME=../usr/X11

d none X11 0755 root bin

!search $HOME/bin
d none X11/bin 0755 root bin


d none X11/lib 0755 root bin
d none X11/lib/X11 0755 root bin

#
# Xprint configuration files
#
d none X11/lib/X11/xserver 0755 root bin
d none X11/lib/X11/xserver/C 0755 root bin
d none X11/lib/X11/xserver/C/print 0755 root bin

!search $HOME/lib/X11/xserver/C/print
f none X11/lib/X11/xserver/C/print/Xprinters 0755 root bin
d none X11/lib/X11/xserver/C/print/attributes 0755 root bin


!search $HOME/lib/X11/xserver/C/print/attributes
f none X11/lib/X11/xserver/C/print/attributes/document 0755 root bin
f none X11/lib/X11/xserver/C/print/attributes/job 0755 root bin
f none X11/lib/X11/xserver/C/print/attributes/printer 0755 root bin

d none X11/lib/X11/xserver/C/print/ddx-config 0755 root bin
d none X11/lib/X11/xserver/C/print/ddx-config/raster 0755 root bin

!search $HOME/lib/X11/xserver/C/print/ddx-config/raster
f none X11/lib/X11/xserver/C/print/ddx-config/raster/pcl 0755 root bin
f none X11/lib/X11/xserver/C/print/ddx-config/raster/postscript 0755 root bin

d none X11/lib/X11/xserver/C/print/models 0755 root bin
d none X11/lib/X11/xserver/C/print/models/HPDJ1600C 0755 root bin
d none X11/lib/X11/xserver/C/print/models/HPDJ1600C/fonts 0755 root bin

!search $HOME/lib/X11/xserver/C/print/models/HPDJ1600C
f none X11/lib/X11/xserver/C/print/models/HPDJ1600C/model-config 0755 root bin

d none X11/lib/X11/xserver/C/print/models/HPLJ4family 0755 root bin
d none X11/lib/X11/xserver/C/print/models/HPLJ4family/fonts 0755 root bin

!search $HOME/lib/X11/xserver/C/print/models/HPLJ4family
f none X11/lib/X11/xserver/C/print/models/HPLJ4family/model-config 0755 root bin

d none X11/lib/X11/xserver/C/print/models/SPSPARC2 0755 root bin
d none X11/lib/X11/xserver/C/print/models/SPSPARC2/fonts 0755 root bin

!search $HOME/lib/X11/xserver/C/print/models/SPSPARC2
f none X11/lib/X11/xserver/C/print/models/SPSPARC2/model-config 0755 root bin

!search $HOME/lib/X11/xserver
f none X11/lib/X11/xserver/README 0755 root bin
